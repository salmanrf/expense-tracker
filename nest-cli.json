{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "**/*.pdf",
        "exclude": "**/omitted.graphql",
        "watchAssets": true
      },
      {
        "include": "**/*.ttf",
        "exclude": "**/omitted.graphql",
        "watchAssets": true
      }
    ]
  }
}
